<html>

<head>
    <style>
        body {
            background-color: aqua;
            font-family: Verdana;
            padding: 50px;
            color: #fff;
        }
        
        h1,
        h2 {
            font-size: 80px;
            text-transform: uppercase;
            letter-spacing: 2px;
            text-align: center;
        }
        
        p,
        li {
            font-size: 60px;
            text-align: center;
        }
    </style>
</head>

<body>
    <h1>Welcome to profile of
        <%= person%>
    </h1>
    <p>Age:
        <%= data.age%>
    </p>
    <p>Job:
        <%= data.job%>
    </p>
    <h2>Hobbies</h2>
    <ul>
        <% data.hobbies.forEach(function(item){ %>
            <li>
                <%= item %>
            </li>
            <% }); %>
    </ul>
</body>

</html>